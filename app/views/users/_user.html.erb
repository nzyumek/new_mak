      <tr>
        <td>
            <%= user.id %>
        </td>
        <td>
            <%= user.gen %>
        </td>
        <td>
            <%= user.lastn %><%= user.firstn %>
        </td>
        <td>
            <%= user.h_lastn %><%= user.h_firstn %>
        </td>
        <td>
            <% if current_user.admin? %>
                <%= user.studno %>
            <% end %>            
        </td>
        <td>
            <%= user.bachelor %>学部
        </td>
        <td>
            <%= user.major %>学科
        </td>
        <td>
            <%= user.email %>
        </td>
        <td>
            <% if user.general? %>
                一般
            <% elsif user.gijutsu_manager? %>
               技術部長
            <% elsif user.admin? %>
                管理員
            <% end %>
        </td>
        <td>
            <% if current_user.admin? %>
                <%= link_to "Edit", edit_user_path(user.id) %>
            <% end %>
        </td>        
      </tr>